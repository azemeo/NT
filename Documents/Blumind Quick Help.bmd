  <nodes>
    <node text="Blumind&#xD;&#xA;Quick Help">
      <style>
        <back_color>Silver</back_color>
        <fore_color>Black</fore_color>
        <border_color>Black</border_color>
        <padding>12</padding>
      </style>
      <widgets>
        <widget type="PICTURE" align="Left" hyperlink=""><![CDATA[]]></widget>
      </widgets>
      <nodes>
        <node text="Create a map" folded="True">
          <nodes>
            <node text="Choose a layout" folded="True">
              <nodes>
                <node text="(Sidebar, Objects) NewDocument" folded="True">
                  <nodes>
                    <node text="(Property, Layout) Layout Type" folded="True" />
                  </nodes>
                </node>
              </nodes>
            </node>
            <node text="Add a topic" folded="True">
              <nodes>
                <node text="Enter" folded="True" />
              </nodes>
            </node>
            <node text="Add a sub topic" folded="True">
              <nodes>
                <node text="Click the Parent Topic" folded="True">
                  <nodes>
                    <node text="Tab" folded="True" />
                  </nodes>
                </node>
              </nodes>
            </node>
            <node text="Edit the topic" folded="True">
              <nodes>
                <node text="Click the Topic" folded="True">
                  <nodes>
                    <node text="Space" folded="True" />
                  </nodes>
                </node>
              </nodes>
            </node>
          </nodes>
        </node>
        <node text="Customize the topics" folded="True">
          <nodes>
            <node text="Change the color" folded="True">
              <style>
                <back_color>YellowGreen</back_color>
              </style>
              <nodes>
                <node text="Click the Topic" folded="True">
                  <nodes>
                    <node text="(Sidebar, Property) Color" folded="True" />
                  </nodes>
                </node>
              </nodes>
            </node>
            <node text="Change the font" folded="True">
              <style>
                <font font_family="Microsoft Sans Serif" size="18" style="Bold, Italic, Underline" />
              </style>
              <nodes>
                <node text="Click the Topic" folded="True">
                  <nodes>
                    <node text="(Sidebar, Property, Appareance) Font" folded="True" />
                  </nodes>
                </node>
              </nodes>
            </node>
            <node text="Add an icon" folded="True">
              <widgets>
                <widget type="PICTURE" align="Left" hyperlink=""><![CDATA[]]></widget>
              </widgets>
              <nodes>
                <node text="Right Click on the Topic" folded="True">
                  <nodes>
                    <node text="Add/Icon" folded="True" />
                  </nodes>
                </node>
              </nodes>
            </node>
            <node text="Add a note" folded="True">
              <widgets>
                <widget type="NOTES" align="Right" hyperlink="">
                  <description>this is a test</description>
                </widget>
              </widgets>
              <nodes>
                <node text="Right Click on the Topic" folded="True">
                  <nodes>
                    <node text="Add/Notes" folded="True" />
                  </nodes>
                </node>
              </nodes>
            </node>
            <node text="Add a progress bar" folded="True">
              <widgets>
                <widget type="PROGRESSBAR" max="100" min="0" value="50" show_text="False" color="Green" back_color="White" fore_color="Black" align="Bottom" hyperlink="" />
              </widgets>
              <nodes>
                <node text="Right Click on the Topic" folded="True">
                  <nodes>
                    <node text="Add/Progresss Bar" folded="True" />
                  </nodes>
                </node>
              </nodes>
            </node>
            <node text="Add a hyperlink" hyperlink="http://www.blumind.org/fr/" folded="True">
              <nodes>
                <node text="Click the Topic" folded="True">
                  <nodes>
                    <node text="(Sidebar, Property) Data\Hyperlink" folded="True" />
                  </nodes>
                </node>
              </nodes>
            </node>
            <node text="Add link between two topics" folded="True">
              <nodes>
                <node text="Click the First Topic" folded="True">
                  <links>
                    <link target="dc8d4604-1955-4e41-8b6c-4083aa67613f" cp1_length="105" cp1_angle="151" cp2_length="71" cp2_angle="0" width="3" line_style="2" start_cap="0" end_cap="20" text="" hyperlink="" />
                  </links>
                  <nodes>
                    <node text="Add Link" folded="True">
                      <nodes>
                        <node id="dc8d4604-1955-4e41-8b6c-4083aa67613f" text="Click the Second Topic" folded="True" />
                      </nodes>
                    </node>
                  </nodes>
                </node>
              </nodes>
            </node>
            <node text="Delete object (e.g. Icon, Note ...)" folded="True">
              <nodes>
                <node text="Select Object" folded="True">
                  <nodes>
                    <node text="Press &quot;Delete&quot; Key" folded="True" />
                  </nodes>
                </node>
                <node text="Right click on object" folded="True">
                  <nodes>
                    <node text="Delete" folded="True" />
                  </nodes>
                </node>
              </nodes>
            </node>
            <node text="Change layout" folded="True">
              <nodes>
                <node text="Click blank area in chart to select chart" folded="True">
                  <nodes>
                    <node text="(Property, Layout) Layout Type" folded="True" />
                  </nodes>
                </node>
              </nodes>
            </node>
          </nodes>
        </node>
        <node text="Organize your map" folded="True">
          <nodes>
            <node text="Move a topic" folded="True">
              <nodes>
                <node text="Select Topic" folded="True">
                  <nodes>
                    <node text="Ctrl+Up/Down" folded="True" />
                  </nodes>
                </node>
                <node text="Right Click on the Topic" folded="True">
                  <nodes>
                    <node text="Move Up" folded="True" />
                    <node text="Move Down" folded="True" />
                  </nodes>
                </node>
                <node text="Right click on the Parent Topic" folded="True">
                  <nodes>
                    <node text="Custom Sort" folded="True" />
                  </nodes>
                </node>
              </nodes>
            </node>
            <node text="Delete a topic" folded="True">
              <nodes>
                <node text="Right Click ont the topic" folded="True">
                  <nodes>
                    <node text="Delete" folded="True" />
                  </nodes>
                </node>
              </nodes>
            </node>
          </nodes>
        </node>
        <node text="View your map" folded="True">
          <nodes>
            <node text="Hide/show subtopics" folded="True">
              <nodes>
                <node text="- (Collapse)" folded="True" />
                <node text="+ (Expand)" folded="True" />
              </nodes>
            </node>
            <node text="Change the view" folded="True">
              <nodes>
                <node text="Full Screen (F11)" folded="True" />
                <node text="Scroll Mode" folded="True" />
              </nodes>
            </node>
          </nodes>
        </node>
        <node text="Save your map" folded="True">
          <widgets>
            <widget type="PICTURE" align="Left" hyperlink="" />
          </widgets>
          <nodes>
            <node text="Menu/Save" folded="True" />
            <node text="Menu/Export" folded="True">
              <nodes>
                <node text="PNG, JPEG, BMP, GIF, TIFF" folded="True" />
                <node text="SVG, Freemind" folded="True" />
                <node text="Text" folded="True" />
              </nodes>
            </node>
          </nodes>
        </node>
        <node text="Print your map" folded="True">
          <nodes>
            <node text="Menu/Print Preview" folded="True">
              <nodes>
                <node text="Print Setup" folded="True" />
              </nodes>
            </node>
            <node text="Menu/Print" folded="True" />
          </nodes>
        </node>
        <node text="More..." folded="True">
          <nodes>
            <node text="Save time" folded="True">
              <nodes>
                <node text="Help/Accelerator Keys Table" folded="True" />
              </nodes>
            </node>
            <node text="Set a theme" folded="True">
              <nodes>
                <node text="Themes/Themes Manage/Preview/Apply Theme" folded="True" />
              </nodes>
            </node>
            <node text="Dowload icons from the Internet" folded="True">
              <widgets>
                <widget type="PICTURE" align="Left" hyperlink="" />
              </widgets>
              <nodes>
                <node text="Add/Icon/From Internet" folded="True" />
              </nodes>
            </node>
            <node text="Create your themes" folded="True">
              <nodes>
                <node text="Themes" folded="True">
                  <nodes>
                    <node text="Themes Manage/New" folded="True" />
                    <node text="Save Current Theme" folded="True" />
                  </nodes>
                </node>
              </nodes>
            </node>
            <node text="Clear/Reset Property" folded="True">
              <nodes>
                <node text="(Property)Right click property" folded="True">
                  <nodes>
                    <node text="&quot;Reset&quot;" folded="True" />
                  </nodes>
                </node>
              </nodes>
            </node>
            <node text="Multi-Line text in Topic" folded="True">
              <nodes>
                <node text="Press &quot;Ctrl+Enter&quot; to start a new line" folded="True" />
              </nodes>
            </node>
          </nodes>
        </node>
      </nodes>
    </node>
  </nodes>
</map>